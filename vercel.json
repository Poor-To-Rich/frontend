{
  "rewrites": [
    {
      "source": "/auth/:match*",
      "destination": "https://api.poortorich.site/auth/:match*"
    },
    {
      "source": "/user/:match*",
      "destination": "https://api.poortorich.site/user/:match*"
    },
    {
      "source": "/expense/:match*",
      "destination": "https://api.poortorich.site/expense/:match*"
    },
    {
      "source": "/income/:match*",
      "destination": "https://api.poortorich.site/income/:match*"
    },
    {
      "source": "/category/:match*",
      "destination": "https://api.poortorich.site/category/:match*"
    },
    {
      "source": "/report/:match*",
      "destination": "https://api.poortorich.site/report/:match*"
    },
    {
      "source": "/chart/:match*",
      "destination": "https://api.poortorich.site/chart/:match*"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
